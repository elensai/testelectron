{
  "name": "electron-project",
  "version": "1.0.3",
  "description": "",
  "main": "index.js",
  "app": "./dist",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1",
    "update-electron-app": "^2.0.1"
  },
  "build": {
    "productName": "eleapp",
    "appId": "org.zero.QuickStart",
    "copyright": "Zero",
    "compression": "normal",
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "runAfterFinish": true
    },
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://electron-update.com/latest.yml"
      }
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  },
  "dependencies": {
    "electron-updater": "^4.3.5"
  }
}
